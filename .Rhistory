if(length(unique(rng)) > 2) {
brks <- ifelse(diff(rng) > 3, '1 year', '4 months')
lab_brks <- ifelse(diff(rng) > 3, '%Y', '%b-%y')
} else {
brks <- ifelse(length(unique(rng)) > 1, '3 months', '1 month')
lab_brks <- '%b-%y'
}
}
